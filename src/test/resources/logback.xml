<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%-4r [%t] %-5p %c %X - %m%n</pattern>
    </encoder>
  </appender>
  <root level="debug">
    <appender-ref ref="console"/>
  </root>
</configuration>
